{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate, max-age=0"
        },
        {
          "key": "Pragma",
          "value": "no-cache"
        },
        {
          "key": "Expires",
          "value": "0"
        },
        {
          "key": "Last-Modified",
          "value": "Thu, 01 Jan 1970 00:00:00 GMT"
        },
        {
          "key": "ETag",
          "value": "\"cache-bust-2025-01-17-logo-fix\""
        }
      ]
    }
  ]
}
